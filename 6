#include <stdio.h>
#include <string.h>

int main() {
    char str1[] = "Nasabayabasan";
    char str2[strlen(str1)];
    int i;
    
    for(i = 0; i < strlen(str1)+1; i++) {
        str2[i] = str1[i];
    }
    
    strrev(str2);
    int x = strcmpi(str1, str2);
    
    
    if (x == 0) {
        printf("The word is a palindrome.");
    }
    else {
        printf("The word is not a palindrome.")
    }
    
    return 0;
}
